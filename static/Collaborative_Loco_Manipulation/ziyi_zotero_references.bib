@inproceedings{budhiraja_dynamics_2019,
	title = {Dynamics {Consensus} between {Centroidal} and {Whole}-{Body} {Models} for {Locomotion} of {Legged} {Robots}},
	doi = {10.1109/ICRA.2019.8793878},
	abstract = {It is nowadays well-established that locomotion can be written as a large and complex optimal control problem. Yet, current knowledge in numerical solver fails to directly solve it. A common approach is to cut the dimensionality by relying on reduced models (inverted pendulum, capture points, centroidal). However it is difficult both to account for whole-body constraints at the reduced level and also to define what is an acceptable trade-off at the whole-body level between tracking the reduced solution or searching for a new one. The main contribution of this paper is to introduce a rigorous mathematical framework based on the Alternating Direction Method of Multipliers, to enforce the consensus between the centroidal state dynamics at reduced and whole-body level. We propose an exact splitting of the whole-body optimal control problem between the centroidal dynamics (under-actuation) and the manipulator dynamics (full actuation), corresponding to a re-arrangement of the equations already stated in previous works. We then describe with details how alternating descent is a good solution to implement an effective locomotion solver. We validate this approach in simulation with walking experiments on the HRP-2 robot.},
	booktitle = {2019 {International} {Conference} on {Robotics} and {Automation} ({ICRA})},
	author = {Budhiraja, Rohan and Carpentier, Justin and Mansard, Nicolas},
	month = may,
	year = {2019},
	note = {ISSN: 2577-087X},
	keywords = {Convex functions, Dynamics, HRP-2 robot, Legged locomotion, Optimal control, Optimization, Trajectory, capture points, centroidal dynamics, centroidal state dynamics, complex optimal control problem, dynamics consensus, effective locomotion, humanoid robots, inverted pendulum, large control problem, legged locomotion, legged robots, manipulator dynamics, mathematical framework, motion control, numerical solver, optimal control, optimisation, pendulums, reduced level, reduced solution, whole-body constraints, whole-body level, whole-body optimal control problem},
	pages = {6727--6733},
}

@inproceedings{dario_bellicoso_dynamic_2017,
	title = {Dynamic locomotion and whole-body control for quadrupedal robots},
	doi = {10.1109/IROS.2017.8206174},
	abstract = {This paper presents a framework which allows a quadrupedal robot to execute dynamic gaits including trot, pace and dynamic lateral walk, as well as a smooth transition between them. Our method relies on an online ZMP based motion planner which continuously updates the reference motion trajectory as a function of the contact schedule and the state of the robot. The planner is coupled with a hierarchical whole-body controller which optimizes the whole-body motion and contact forces by solving a cascade of prioritized tasks. We tested our framework on ANYmal, a fully torque controllable quadrupedal robot which is actuated by series-elastic actuators.},
