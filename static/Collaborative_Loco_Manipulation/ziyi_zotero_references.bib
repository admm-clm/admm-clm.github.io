@article{wahrburg_motor-current-based_2018,
	title = {Motor-{Current}-{Based} {Estimation} of {Cartesian} {Contact} {Forces} and {Torques} for {Robotic} {Manipulators} and {Its} {Application} to {Force} {Control}},
